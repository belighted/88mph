%h1 Login

- form_for @user_session, :url => user_session_path do |f|
  = f.error_messages
  %ol
    %li
      = f.label :email
      = f.text_field :email
    %li
      = f.label :password
      = f.password_field :password
    %li
      = f.check_box :remember_me
      = f.label :remember_me
  = f.submit "Login"