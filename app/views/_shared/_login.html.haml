-form_for :user_session, :url => user_session_path do |f|
  %ol
    %li
      = f.label :email, "email"
      = f.text_field :email
    %li
      = f.label :password, "password"
      = f.password_field :password
    %li
      = f.check_box :remember_me
      = f.label :remember_me, "rm"
  %p
    = f.submit "login"